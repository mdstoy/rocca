{
   "manifest_version": 2,
   "name": "Rocca",
   "default_locale": "en_US",
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "a rocker gesture extention",
   "permissions": [ "tabs" ],
   "version": "0.0.1"
}
