{
	"manifest_version": 2,
	"name": "Rocca",
	"version": "0.0.9",

	"default_locale": "en_US",
	"description": "a rocker gesture extention",

	"background": {
		"scripts": [ "background.js" ],
		"persistent": false
	},
	"content_scripts": [ {
		"js": [ "script.js" ],
		"matches": [ "http://*/*", "https://*/*" ]
	} ],
	"permissions": [ "tabs" ]
}

